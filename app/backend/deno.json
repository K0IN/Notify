    "lint": {
        "include": [
            "./**/*.ts"
        ],
        "exclude": [],
        "rules": {
            "tags": [
                "recommended"
            ],
            "include": [],
            "exclude": [
                "ban-untagged-todo",
                "import-prefix-missing"
            ]
        }
    }
