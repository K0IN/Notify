{
    "name": "notify worker",
    "version": "1.0.0",
    "description": "Notify worker",
    "main": "dist/worker.js",
    "scripts": {
        "build_all": "webpack && cd frontend && npm install && npm run build",
        "build": "webpack",
        "serve": "miniflare ./dist/worker.js --kv-persist --wrangler-config wrangler.toml",
        "watch": "miniflare ./dist/worker.js --kv-persist --wrangler-config wrangler.toml --watch",
        "run_docker": "miniflare ./dist/worker.js --kv-persist --wrangler-config wrangler.toml --upstream localhost"
    },
    "eslintConfig": {
        "root": true,
        "extends": [
            "typescript",
            "prettier"
        ]
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^2.2.2",
        "@types/web-push": "^3.3.2",
        "miniflare": "^1.4.0",
        "ts-loader": "^9.2.2",
        "typescript": "^4.3.2",
        "webpack": "^5.51.1",
        "webpack-cli": "^4.7.0"
    },
    "dependencies": {
        "@cloudflare/kv-asset-handler": "^0.1.3",
        "itty-router": "^2.4.2"
    }
}
